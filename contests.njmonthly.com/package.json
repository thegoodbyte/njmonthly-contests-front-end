{
  "name": "vue-contests",
  "version": "1.0.0",
  "description": "A Vue.js SSR project",
  "author": "",
  "private": true,
  "scripts": {
    "dev": "nodemon -L server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "npm run build:client && npm run build:server && rimraf dist && node build/rename.js",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build:single": "npm run build:client && rimraf dist && node build/rename.js",
    "add:component": "cd src/components && vue init billwing/vue-ssr-component .",
    "add:view": "cd src/views && vue init billwing/vue-ssr-view"
  },
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
  "dependencies": {
    "algoliasearch": "^3.35.1",
    "axios": "^0.21.1",
    "bootstrap-sass": "^3.4.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^5.2.1",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.21",
    "lru-cache": "^4.1.3",
    "medium-zoom": "^1.0.6",
    "nodemon": "^1.19.4",
    "places.js": "^1.19.0",
    "qs": "^6.10.1",
    "query-string": "^6.14.1",
    "route-cache": "0.4.4",
    "serve-favicon": "^2.5.0",
    "smoothscroll": "^0.4.0",
    "vue": "^2.6.12",
    "vue-analytics": "^5.22.1",
    "vue-cookie": "^1.1.4",
    "vue-meta": "^1.6.0",
    "vue-no-ssr": "^1.0.0",
    "vue-router": "^3.5.1",
    "vue-select": "^2.6.4",
    "vue-server-renderer": "^2.6.12",
    "vue-youtube": "^1.4.0",
    "vue-youtube-embed": "^2.2.2",
    "vue2-leaflet": "^1.1.1",
    "vuejs-paginate": "^2.0.1",
    "vuex": "^3.6.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "chokidar": "^2.1.8",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.7.1",
    "sass-loader": "^7.3.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.1.1",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
